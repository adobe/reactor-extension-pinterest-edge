{
  "name": "@adobe/reactor-extension-pinterest-edge",
  "platform": "edge",
  "version": "1.0.1",
  "description": "Forward data to Pinterest by using the Conversion API.",
  "author": {
    "name": "Adobe",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-extension-pinterest-edge.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run clean && npm run build && npx @adobe/reactor-packager",
    "prepack": "npm run clean && npm run build",
    "lint": "eslint 'src/**'",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i",
    "prepare": "husky install"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.27.0",
    "@spectrum-icons/illustrations": "^3.6.2",
    "@spectrum-icons/workflow": "^4.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-stately": "^3.23.0"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^12.5.0",
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/runtime": "^7.22.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "camelcase": "^7.0.1",
    "capitalize": "^2.0.4",
    "concurrently": "^8.1.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-runner-eslint": "^2.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "style-loader": "^3.3.3",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "yargs": "^17.7.2"
  }
}
